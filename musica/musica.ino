int res = 0;
void setup() {
	// Allow allocation of all timers
	ESP32PWM::allocateTimer(0);
	ESP32PWM::allocateTimer(1);
	ESP32PWM::allocateTimer(2);
	ESP32PWM::allocateTimer(3);
	Serial.begin(9600);
}

void loop() 
{
  check();
}


void check(){
  while(Serial.available()){
    valor_tiva = Serial.read();
  }
  valor_tiva = Serial.read();

  if (valor_tiva == 1){
    if (res != 1){
      res = 1;
      menu();
    }
    }

  else if (valor_tiva == 2){
    if (res != 2){
      res = 2;
    fight();
  }
  }
  else if (valor_tiva == 3){
    if (res != 3){
      res = 3;
    primero();
  }
}
}

